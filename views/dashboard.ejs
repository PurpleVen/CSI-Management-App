              <script>
                $(document).ready(function () {
                  load_data();
                  function load_data() {
                    $.ajax({
                      url: "/confirmedall",
                      method: "GET",
                      dataType: "JSON",
                      success: function (data) {

                        var html = '';
                        var proposals_three_track = '';
                        var color = ' ';
                        if (data.data.length > 0) {

                          for (var count = 0; count < data.data.length; count++) {

                            if (data.data[count].proposals_three_track == 1) {
                              proposals_three_track = 1;
                              color = '#00ccff';
                            }
                            else if (data.data[count].proposals_three_track == 3) {
                              proposals_three_track = 3;
                              color = '#66ff99';
                            }
                            else if (data.data[count].proposals_three_track == 2) {
                              proposals_three_track = 2;
                              color = '#ff66ff';
                            }

                            // if (category == 'Aspiration'){
                            //   color = 'cyan'
                            // }

                            html += `
                                      <tr>
                                          <td style="vertical-align: middle; text-align: center;color: black;">`+ data.data[count].proposals_event_name + `</td>
                                          <td style="vertical-align: middle; text-align: center;"> <span class="badge badge-sm" style="background-color:`+ color + `; color: black;">` + proposals_three_track + `</span></td>
                                          

                                      </tr>
                                      `;
                          }
                        }
                        $('tbody').html(html);

                      },
                      error: function (XMLHttpRequest, textStatus, errorThrown) {
                        console.log(error);
                        console.log("confirmedall  inside AJAX function: Err");
                        alert("Status: " + textStatus); alert("Error: " + errorThrown);
                      }
                    });
                  }
                });
              </script>
